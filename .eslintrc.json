{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "prettier",
    "airbnb-base",
    "plugin:@typescript-eslint/recommended",
    "plugin:import/recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-console": "off",
    "import/no-unresolved": "off",
    "import/extensions": ["off"]
  }
}
